			<string>match parameters inside handlebars block/inline functions</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>begin</key>
					<string>\s</string>
					<key>end</key>
					<string>(?=\s|\})</string>
					<key>patterns</key>
					<array>
						<dict>
							<key>comment</key>
							<string>any identifier between two spaces (or a space and a closing brace)</string>
							<key>match</key>
							<string>(?&lt;=\s)[a-zA-Z0-9\.\/]+(?=\s|\})</string>
							<key>name</key>
							<string>variable.parameter.function.handlebars</string>
						</dict>
						<dict>
							<key>begin</key>
							<string>([a-zA-Z0-9\.\/]+)(=)</string>
							<key>beginCaptures</key>
							<dict>
								<key>1</key>
								<dict>
									<key>name</key>
									<string>variable.parameter.function.keyword.handlebars</string>
								</dict>
								<key>2</key>
								<dict>
									<key>name</key>
									<string>punctuation.parameter.function.keyword.handlebars</string>
								</dict>
							</dict>
							<key>comment</key>
							<string>a keyword argument and its value</string>
							<key>end</key>
							<string>(?=\s|\})</string>
							<key>patterns</key>
							<array>
								<dict>
									<key>include</key>
									<string>#string-single-quoted</string>
								</dict>
								<dict>
									<key>include</key>
									<string>#string-double-quoted</string>
								</dict>
							</array>
						</dict>
						<dict>
							<key>begin</key>
							<string>(?&lt;=\s)</string>
							<key>comment</key>
							<string>any string between two spaces (or a space and a closing brace)</string>
							<key>end</key>
							<string>(?=\s|\})</string>
							<key>patterns</key>
							<array>
								<dict>
									<key>include</key>
									<string>#string-single-quoted</string>
								</dict>
								<dict>
									<key>include</key>
									<string>#string-double-quoted</string>
								</dict>
							</array>
						</dict>
					</array>
				</dict>
			</array>
		</dict>
