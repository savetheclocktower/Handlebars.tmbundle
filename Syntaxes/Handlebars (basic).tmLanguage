	<array/>
	<key>name</key>
	<string>Handlebars</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>include</key>
			<string>#handlebars-blocks</string>
		</dict>
		<dict>
			<key>include</key>
			<string>#handlebars-inline</string>
		</dict>
	</array>
	<key>repository</key>
	<dict>
